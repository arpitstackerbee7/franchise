!--    </thead>-->\n<!--    <tbody>-->\n<!--        {% for item in doc.items %}-->\n<!--        <tr>-->\n<!--            <td align=\"center\">Assembly Items</td>-->\n<!--            <td><b>{{ item.item_code }}</b><br><small>{{ item.item_name }}</small></td>-->\n<!--            <td align=\"center\">{{ item.gst_hsn_code or item.hsn_code or '' }}</td>-->\n<!--            <td align=\"center\">{{ doc.purchase_order or '' }}</td>-->\n<!--            <td align=\"center\">{{ item.get_formatted(\"schedule_date\") }}</td>-->\n<!--            <td align=\"right\">{{ item.get_formatted(\"rate\") }}</td>-->\n<!--            <td align=\"right\"><b>{{ item.get_formatted(\"qty\") }}</b></td>-->\n<!--            <td align=\"center\">{{ item.uom or '' }}</td>-->\n<!--            <td align=\"right\">{{ item.get_formatted(\"amount\") }}</td>-->\n<!--        </tr>-->\n<!--        {% endfor %}-->\n<!--        <tr class=\"total-row\">-->\n<!--            <td colspan=\"6\" align=\"right\">Assembly Item Total</td>-->\n<!--            <td align=\"right\">{{ doc.get_formatted(\"total_qty\") }}</td>-->\n<!--            <td></td>-->\n<!--            <td align=\"right\">{{ doc.get_formatted(\"total\") }}</td>-->\n<!--        </tr>-->\n<!--    </tbody>-->\n<!--</table>-->\n\n<!-- Component Items Table -->\n<!--<table class=\"data-table\">-->\n<!--    <thead>-->\n<!--        <tr class=\"section-head\">-->\n<!--            <th width=\"15%\">Type</th>-->\n<!--            <th width=\"65%\">Group/Item</th>-->\n<!--            <th width=\"10%\">Qty.</th>-->\n<!--            <th width=\"10%\">UOM</th>-->\n<!--        </tr>-->\n<!--    </thead>-->\n<!--    <tbody>-->\n<!--        {% for s_item in doc.supplied_items %}-->\n<!--        <tr>-->\n<!--            <td align=\"center\">Component Items</td>-->\n<!--            <td>-->\n<!--                <b>{{ s_item.rm_item_code }}</b><br>-->\n<!--                <small>{{ s_item.item_name if s_item.item_name else '' }}</small>-->\n<!--            </td>-->\n<!--            <td align=\"right\"><b>{{ s_item.get_formatted(\"required_qty\") }}</b></td>-->\n<!--            <td align=\"center\">{{ s_item.uom or s_item.stock_uom or '' }}</td>-->\n<!--        </tr>-->\n<!--        {% endfor %}-->\n<!--        <tr class=\"total-row\">-->\n<!--            <td colspan=\"2\" align=\"right\">Component Item Total</td>-->\n<!--            <td align=\"right\">-->\n<!--                {% set total_comp = doc.supplied_items | sum(attribute='required_qty') %}-->\n<!--                {{ \"{:,.3f}\".format(total_comp) }}-->\n<!--            </td>-->\n<!--            <td></td>-->\n<!--        </tr>-->\n<!--    </tbody>-->\n<!--</table>-->\n\n<!-- Footer Section -->\n<!--<div class=\"footer-section\">-->\n<!--    <div style=\"text-align: right; font-weight: bold; margin-bottom: 60px;\">For {{ doc.company }}</div>-->\n<!--    <div class=\"sig-box\">Prepared By</div>-->\n<!--    <div class=\"sig-box\">Checked By</div>-->\n<!--    <div class=\"sig-box\" style=\"margin-right:0;\">Authorized Signatory</div>-->\n<!--</div>-->\n\n<!-- Header Section -->\n<div class=\"print-header\">\n    <table width=\"100%\" cellpadding=\"0\" cellspacing=\"0\" style=\"table-layout: fixed;\">\n        <tr>\n            <!-- Left Side: Logo and Company Info -->\n            <td width=\"60%\" style=\"vertical-align: bottom;\">\n                <!-- Logo -->\n                <img src=\"/files/WhatsApp%20Image%202026-01-12%20at%2011.57.36%20AM.jpeg\" \n                     style=\"max-height: 65px; width: auto; display: block; margin-bottom: 8px;\">\n                \n                <div class=\"company-name\">{{ doc.company }}</div>\n                <div style=\"font-size: 12px; line-height: 1.2;\">\n                    52, Gali doman, Bagh Diwar, MCD Parking,<br>\n                    Near Fatehpuri, Church Road, Chandni Chowk, DELHI, 110006\n                </div>\n\n                <!-- CIN & GSTIN Perfect Alignment -->\n                <table style=\"font-size: 10px; border: none !important; width: auto; border-collapse: collapse;\">\n                    <tr>\n                        <td style=\"border:none !important; padding:0 !important; width: 45px;\">CIN</td>\n                        <td style=\"border:none !important; padding:0 !important; width: 10px;\">:</td>\n                        <td style=\"border:none !important; padding:0 !important;\">{{ frappe.db.get_value(\"Company\", doc.company, \"tax_id\") or '' }}</td>\n                    </tr>\n                    <tr>\n                        <td style=\"border:none !important; padding:0 !important;\">GSTIN</td>\n                        <td style=\"border:none !important; padding:0 !important;\">:</td>\n                        <td style=\"border:none !important; padding:0 !important;\">{{ frappe.db.get_value(\"Company\", doc.company, \"gstin\") or '' }}[{{ doc.place_of_supply or '' }}]</td>\n                    </tr>\n                </table>\n            </td>\n            \n            <!-- Right Side: Order No & Date Perfect Alignment -->\n            <td width=\"40%\" class=\"title-box\" style=\"vertical-align: bottom; padding-bottom: 5px;\">\n                <span style=\"font-size: 18px; display: block; margin-bottom: 8px; text-align: right;\">JOB ORDER FINAL</span>\n                \n                <table style=\"width: 100%; border: none !important; border-collapse: collapse; font-size: 11px; line-height: 1.2;\">\n                    <tr>\n                        <td style=\"border: none !important; padding: 1px 0; text-align: right; white-space: nowrap;\"><b>Order No.</b></td>\n                        <td style=\"border: none !important; padding: 1px 5px; width: 10px; text-align: center;\">:</td>\n                        <td style=\"border: none !important; padding: 1px 0; text-align: left; width: 120px;\"><b>{{ doc.name }}</b></td>\n                    </tr>\n                    <tr>\n                        <td style=\"border: none !important; padding: 1px 0; text-align: right; white-space: nowrap;\"><b>Date</b></td>\n                        <td style=\"border: none !important; padding: 1px 5px; text-align: center;\">:</td>\n                        <td style=\"border: none !important; padding: 1px 0; text-align: left;\"><b>{{ doc.get_formatted(\"transaction_date\") }}</b></td>\n                    </tr>\n                </table>\n            </td>\n        </tr>\n    </table>\n</div>\n\n<!-- JOB ORDER FINAL Central Title -->\n<div class=\"central-job-title\">\n    JOB ORDER FINAL\n</div>\n\n Jobber & Ref Details \n<table class=\"info-table\">\n    <tr>\n        <td width=\"55%\">\n            <b>Jobber Name &nbsp;&nbsp;&nbsp;&nbsp;&nbsp; :</b> <b>{{ doc.supplier_name }}</b><br>\n            \n            {% set addr_ref = doc.supplier_address or frappe.db.get_value(\"Dynamic Link\", {\"link_doctype\": \"Supplier\", \"link_name\": doc.supplier, \"parenttype\": \"Address\"}, \"parent\") %}\n            \n            <b>Address &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; :</b> {{ frappe.db.get_value(\"Address\", addr_ref, \"address_line1\") or '' }}<br>\n            <b>Phone No &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; :</b> {{ frappe.db.get_value(\"Address\", addr_ref, \"phone\") or '' }}<br>\n            <b>Party GSTIN &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; :</b> {{ doc.supplier_gstin or frappe.db.get_value(\"Supplier\", doc.supplier, \"gstin\") or '' }}<br>\n            <b>Party GST State :</b> {{ doc.place_of_supply or '' }}\n        </td>\n        <td width=\"45%\">\n            <b>Ref No &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;:</b> {{ doc.purchase_order or '' }}<br>\n            <b>Ref Date &nbsp;&nbsp;&nbsp;&nbsp;:</b> {{ doc.get_formatted(\"transaction_date\") }}<br>\n            <b>Job Type &nbsp;&nbsp;&nbsp;:</b> <br>\n            <b>Process &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;:</b><br>\n            <b>Remarks &nbsp;&nbsp;&nbsp;&nbsp;:</b> {{ doc.remarks or 'FABRIC FOR COSTING' }}\n        </td>\n    </tr>\n</table>\n\n<!-- Assembly Items Table -->\n<table class=\"data-table\">\n    <thead>\n        <tr>\n            <th width=\"10%\">Type</th>\n            <th width=\"25%\">Group/Item</th>\n            <th width=\"10%\">HSN Code</th>\n            <th width=\"15%\">Plan No</th>\n            <th width=\"12%\">Due Date</th>\n            <th width=\"10%\">Rate</th>\n            <th width=\"6%\">Qty.</th>\n            <th width=\"4%\">UOM</th>\n            <th width=\"8%\">Amount</th>\n        </tr>\n    </thead>\n    <tbody>\n        {% for item in doc.items %}\n        <tr>\n            <td align=\"center\">Assembly Items</td>\n            <td align=\"center\"><b>{{ item.item_code }}</b><br><small>{{ item.item_name }}</small></td>\n            <td align=\"center\">{{ item.gst_hsn_code or item.hsn_code or '' }}</td>\n            <td align=\"center\">{{ item.bom or '' }}</td>\n            <td align=\"center\">{{ item.get_formatted(\"schedule_date\") }}</td>\n            <td align=\"right\" style=\"padding-top: 3px !important;\" >{{ item.get_formatted(\"rate\") }}</td>\n            <td align=\"right\" style=\"text-align: center;\" ><b>{{ item.get_formatted(\"qty\") }}</b></td>\n            <td align=\"center\">{{ item.stock_uom or '' }}</td>\n            <td align=\"right\" style=\"padding: 0px !important;padding-top: 4px !important;\" >{{ item.get_formatted(\"amount\") }}</td>\n        </tr>\n        {% endfor %}\n        <tr class=\"total-row\"  style=\"background-color: #eeeeee !important; border-top: 1px solid black; border-bottom: 1px solid black;\">\n            <td colspan=\"5\" align=\"right\" style=\"vertical-align: middle !important;\" >Assembly Item Total</td>\n            <td  align=\"right\"></td>\n            <td  style=\"vertical-align: middle !important; text-align: center;\" align=\"right\">{{ doc.get_formatted(\"total_qty\") }}</td>\n            <td colspan=\"2\" align=\"right\">{{ doc.get_formatted(\"total\") }}</td>\n        </tr>\n    </tbody>\n</table>\n\n<!-- Component Items Table -->\n<table class=\"data-table\" style=\"margin-top:15px;\">\n    <thead>\n        <tr>\n            <th width=\"15%\">Type</th>\n            <th width=\"65%\">Group/Item</th>\n            <th width=\"10%\">Qty.</th>\n            <th width=\"10%\">UOM</th>\n        </tr>\n    </thead>\n    <tbody>\n        {% for s_item in doc.supplied_items %}\n        <tr>\n            <td align=\"center\"><b>Component Items</b></td>\n            <td align=\"center\"><b>{{ s_item.rm_item_code }}</b><br><small>{{ s_item.item_name or '' }}</small></td>\n            <td align=\"right\" style=\"text-align: center;\" ><b>{{ s_item.get_formatted(\"required_qty\") }}</b></td>\n            <td align=\"center\">{{ s_item.uom or s_item.stock_uom or '' }}</td>\n        </tr>\n        {% endfor %}\n        <tr class=\"total-row\" style=\"background-color: #eeeeee !important; border-top: 1px solid black; border-bottom: 1px solid black;\">\n            <td colspan=\"2\" align=\"right\">Component Item Total</td>\n            <td align=\"center\">\n                {% set total_comp = doc.supplied_items | sum(attribute='required_qty') %}\n                {{ \"{:,.3f}\".format(total_comp) }}\n            </td>\n            <td></td>\n        </tr>\n    </tbody>\n</table>\n\n<!-- Footer Section -->\n<div class=\"footer-section\" style=\"margin-top: 30px;\">\n    <div style=\"text-align: right; font-weight: bold; margin-bottom: 5px;\">\n        For {{ doc.company }}\n    </div>\n    <!--Company Sign-->\n    <div style=\"text-align: right; height: 80px; margin-bottom: 5px;\">\n        {% set sign_logo = frappe.db.get_value(\"Company\", doc.company, \"custom_sign_logo\") %}\n        <img src=\"\" \n             style=\"max-height: 80px; width: auto; margin-right: 20px;\">\n    </div>\n    <div class=\"sig-box\">Prepared By</div>\n    <div class=\"sig-box\">Checked By</div>\n    <div class=\"sig-box\" style=\"margin-right:0;\">Authorized Signatory</div>\n</div>",
